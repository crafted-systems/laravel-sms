{
  "name": "crafted-systems/laravel-sms",
  "description": "A Laravel Package for Sending out SMS from multiple Gateways",
  "license": "MIT",
  "type": "library",
  "require": {
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "authors": [
    {
      "name": "Vincent Mosoti",
      "email": "vincent@crafted.co.ke",
      "homepage": "https://github.com/vmosoti",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "CraftedSystems\\LaravelSMS\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "CraftedSystems\\LaravelSMS\\SMSServiceProvider"
      ],
      "aliases": {
        "SMS": "CraftedSystems\\LaravelSMS\\Facades\\SMS"
      }
    }
  },
  "minimum-stability": "dev"
}
